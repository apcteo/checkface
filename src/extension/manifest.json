{
  "manifest_version": 2,
  "name": "CheckFace",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css":["static/css/bootstrap.min.css"],
      "js": ["static/js/jquery-2.2.4.js","static/js/bootstrap.min.js", "content.js"]
    }
  ],
  "browser_action": {
  "default_icon": "static/images/checkface-icon.png"
  },
  "background": {
  "scripts": ["background.js"]
  },
  "permissions": [
         "contextMenus"
       ],
      "icons": {
         "16": "static/images/checkface-icon.png"
       }
}
